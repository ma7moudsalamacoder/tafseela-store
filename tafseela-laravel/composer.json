{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.31",
        "laravel/sanctum": "^4.2",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "lorisleiva/laravel-actions": "^2.9",
        "nwidart/laravel-modules": "^12.0",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-permission": "^6.24"
        
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\Admin\\": "Modules/Admin/app/",
            "Modules\\Admin\\Database\\Factories\\": "Modules/Admin/database/factories/",
            "Modules\\Admin\\Database\\Seeders\\": "Modules/Admin/database/seeders/",
            "Modules\\Cart\\": "Modules/Cart/app/",
            "Modules\\Cart\\Database\\Factories\\": "Modules/Cart/database/factories/",
            "Modules\\Cart\\Database\\Seeders\\": "Modules/Cart/database/seeders/",
            "Modules\\Delivery\\": "Modules/Delivery/app/",
            "Modules\\Delivery\\Database\\Factories\\": "Modules/Delivery/database/factories/",
            "Modules\\Delivery\\Database\\Seeders\\": "Modules/Delivery/database/seeders/",
            "Modules\\Order\\": "Modules/Order/app/",
            "Modules\\Order\\Database\\Factories\\": "Modules/Order/database/factories/",
            "Modules\\Order\\Database\\Seeders\\": "Modules/Order/database/seeders/",
            "Modules\\Product\\": "Modules/Product/app/",
            "Modules\\Product\\Database\\Factories\\": "Modules/Product/database/factories/",
            "Modules\\Product\\Database\\Seeders\\": "Modules/Product/database/seeders/",
            "Modules\\Core\\": "Modules/Core/app/",
            "Modules\\Core\\Database\\Factories\\": "Modules/Core/database/factories/",
            "Modules\\Core\\Database\\Seeders\\": "Modules/Core/database/seeders/",
            "Modules\\Identity\\": "Modules/Identity/app/",
            "Modules\\Identity\\Database\\Factories\\": "Modules/Identity/database/factories/",
            "Modules\\Identity\\Database\\Seeders\\": "Modules/Identity/database/seeders/",
            "Modules\\Payment\\": "Modules/Payment/app/",
            "Modules\\Payment\\Database\\Factories\\": "Modules/Payment/database/factories/",
            "Modules\\Payment\\Database\\Seeders\\": "Modules/Payment/database/seeders/",
            "Modules\\Support\\": "Modules/Support/app/",
            "Modules\\Support\\Database\\Factories\\": "Modules/Support/database/factories/",
            "Modules\\Support\\Database\\Seeders\\": "Modules/Support/database/seeders/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
